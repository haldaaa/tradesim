{
  "dashboard": {
    "id": null,
    "title": "TradeSim - Événements & Métriques Avancées",
    "tags": ["tradesim", "evenements"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Événements Totaux",
        "type": "stat",
        "targets": [
          {
            "expr": "tradesim_events_total",
            "legendFormat": "Événements"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "gradient-gauge"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Latence Moyenne (ms)",
        "type": "stat",
        "targets": [
          {
            "expr": "tradesim_latency_average_ms",
