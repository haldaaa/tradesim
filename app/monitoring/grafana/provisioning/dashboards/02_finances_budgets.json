{
  "dashboard": {
    "id": null,
    "title": "TradeSim - Finances & Budgets",
    "tags": ["tradesim", "finances"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Budget Total (€)",
        "type": "stat",
        "targets": [
          {
            "expr": "tradesim_budget_total_euros",
            "legendFormat": "Budget Total"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "gradient-gauge"
            },
            "unit": "currencyEUR",
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 5000},
                {"color": "green", "value": 10000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Budget Moyen par Entreprise (€)",
